{
  "files": [
    "server.ts"
  ],
  "quiet": false,
  "debug": true,
  "fullscreen": true,
  "extensions": [
    ".js",
    ".ts"
  ],
  "interval": 500,
  "watch": [
    "./"
  ],
  "deno_args":[
    "--allow-net"
  ],
  "execute": {
    ".js": ["deno", "run"],
    ".ts": ["deno", "run"]
  },
  "fmt": false,
  "test": true
}